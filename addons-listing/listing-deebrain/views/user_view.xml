<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <data>
        <record id="users_model_form_view" model="ir.ui.view">
            <field name="name">users_model.form</field>
            <field name="model">users_model</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="founderName" widget="many2many_tags"/>
                                <field name="numberOfEmployees"/>
                                <field name="foundationYear"/>
                                <field name="agencyType"/>
                                <field name="sales"/>
                                <field name="logo" widget='image'/>
                            </group>
                            <group>
                                <field name="planning"/>
                                <field name="project"/>
                                <field name="task"/>
                                <field name="platform"/>
                                <field name="reporting"/>
                                <field name="dataAnalysis"/>
                                <field name="adServer"/>
                                <field name="AdVerification" />
                            </group>
                            <group>
                                <field name="managedBilling"/>
                                <field name="awareness"/>
                                <field name="conversion"/>
                                <field name="consideration"/>
                                <field name="retention"/>
                                <field name="advocacy"/>
                                <field name="website"/>
                                <field name="linkedinLink"/>
                                <field name="facebookLink"/>
                                <field name="email"/>
                            </group>
                            <group>
                                <field name="locations" widget="many2many_tags"/>
                                <field name="locationsNotItaly" widget="many2many_tags"/>
                                <field name="languages" widget="many2many_tags"/>
                                <field name="mainServices" widget="many2many_tags"/>
                                <field name="distinctiveServices" widget="many2many_tags"/>
                            </group>
                            <group>
                                <field name="managedMedia" widget="many2many_tags"/>
                                <field name="managedPlatform" widget="many2many_tags"/>
                                <field name="referralClient" widget="many2many_tags"/>
                            </group>
                            <group>
                                <field name="brochure" widget='file'/>
                                <field name="caseStudy" widget='file'/>
                                <field name="clientLogos" widget="many2many_tags"/>
                                <field name="typeAcc" />
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="users_model_referral_form_view" model="ir.ui.view">
            <field name="name">users_referral_client.form</field>
